<%- include("partials/header") %>


<header class="jumbotron" >
	<div class="container">
		
	<h1 style="text-align: center;">
	The Janhit Welfare Association
	</h1>
	
	</div>

</header>

<div class="container" style="padding-top:50px; margin-bottom: 50px">
	
<div class="row">
	<div class="col-md-2 col-sm-2 col-xs-2">
		
	</div>
	<div class="col-md-3 col-sm-3 col-xs-3">
			<a href="/register" class="btn btn-warning btn-lg">
				Register User
			</a>
	</div>
	
	<div class="col-md-3 col-sm-3 col-xs-3">
			<a href="/loginuser" class="btn btn-success btn-lg">
				Login For User
			</a>
	</div>
	
	<div class="col-md-3 col-sm-3 col-xs-3">
			<a href="/loginadmin" class="btn btn-danger btn-lg">
				Login for Admin
			</a>
	</div>
	
	<div class="col-md-1 col-sm-1 col-xs-1">
		
	</div>	
	
</div>
</div>

<div class="row text-center" style="display:flex; flex-wrap:wrap;">
    <% images.forEach(function(image){ %>
		<div class="col-md-3 col-sm-3 ">
			<div class="thumbnail">
				<img  src= "data:image/<%=image.img.contentType%>;base64, 
                     <%=image.img.data.toString('base64')%>" class="d-block w-100" >
				<div class="caption">
					<h2> <%= image.author %> </h2>
				</div>
			</div>
		</div>
      		
	<% }); %>
</div>

<%- include("partials/footer") %>

